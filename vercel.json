{
    "version": 2,
    "buildCommand": "npm install && npm run build --workspace=@arcanum/web",
    "outputDirectory": "apps/web/dist",
    "installCommand": "npm install",
    "framework": "vite",
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}